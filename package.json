  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.6.0",
    "dot": "^1.1.3",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^5.11.14",
    "react-moment": "^1.1.1"
  }
}
